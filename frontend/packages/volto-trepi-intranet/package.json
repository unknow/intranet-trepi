    "classnames": "^2.5.1",
    "react-intl": "^3.12.1",
    "@plone/registry": "workspace:*",
    "@plone/types": "workspace:*"
  },
  "devDependencies": {
    "@plone/scripts": "workspace:*",
    "release-it": "^19.0.5",
    "@testing-library/react": "^16.2.0",
    "vitest": "^3.1.2"
  }
